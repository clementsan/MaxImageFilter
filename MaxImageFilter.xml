<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>UNC</category>
  <title>MaxImageFilter</title>
  <description> Maximum image filter: The pixel values of the output image are the maximum between the corresponding pixels of the two input images.</description>
  <version>1.0</version>
  <documentation-url></documentation-url>
  <license></license>
  <contributor>Cedric P Mathieu</contributor>
  <acknowledgements>ITK</acknowledgements> 

  <parameters>
    <label>Input</label>
    <description>Choose 2 input images</description>
    
    <image>
      <name>filename1</name>
      <label>Image 1</label>
      <channel>input</channel>
      <index>0</index>
      <description>First input image</description>
    </image>

    <image>
      <name>filename2</name>
      <label>Image 2</label>
      <channel>input</channel>
      <index>1</index>
      <description>Second input image</description>
    </image>
  </parameters>
  
  <parameters>
    <label>Output</label>
    <description>Output image</description>
    <image>
      <name>outputfilename</name>
      <label>Output image</label>
      <flag>o</flag>
      <longflag>outputimage</longflag>
      <channel>output</channel>
      <description>Output Image</description>
    </image>
  </parameters>

</executable>
